# Copy this file to `.env` for local development
# On Render, set these in the Dashboard > Environment

# MongoDB connection string
MONGO_URI=

# Server port (Render injects PORT; locally you can set 5000)
PORT=5000

# Environment
NODE_ENV=development

# CORS allowed frontend origin
# Example: https://your-frontend.onrender.com or http://localhost:5173
FRONTEND_URL=